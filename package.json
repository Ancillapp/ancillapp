{
  "name": "ancillapp",
  "version": "0.0.0",
  "description": "Ancillapp",
  "author": "Giorgio Garasto <giorgio@garasto.it>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "lerna run start --stream",
    "build": "lerna run build --stream",
    "start:pwa": "npm --prefix pwa start",
    "build:pwa": "npm --prefix pwa run build",
    "start:functions": "npm --prefix functions start",
    "build:functions": "npm --prefix functions run build",
    "postinstall": "lerna bootstrap --hoist --strict && patch-package"
  },
  "devDependencies": {
    "firebase-tools": "^8.2.0",
    "lerna": "^3.20.2",
    "lighthouse": "6.0.0-beta.0",
    "node-fetch": "^2.6.0",
    "node-telegram-bot-api": "^0.40.0",
    "patch-package": "^6.2.2",
    "puppeteer": "^3.0.2",
    "typescript": "^3.8.3"
  }
}
